      "source": [
        "# import module\n",
        "from sklearn.preprocessing import MinMaxScaler\n",
        "\n",
        "# create data\n",
        "x = [[110, 2], [300, 7], [0, 10], [110, 8]]\n",
        "print(x)\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# scale features\n",
        "scaler = MinMaxScaler()\n",
        "model=scaler.fit(x)\n",
        "scaled_x=model.transform(x)\n",
        "\n",
        "# print scaled features\n",
        "print(scaled_x)"
      ],
